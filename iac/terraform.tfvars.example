# Terraform Variables Example
# Copy this file to terraform.tfvars and fill in your actual values
# NEVER commit terraform.tfvars to version control

# Required: Vercel API Token
# Get this from: https://vercel.com/account/tokens
vercel_api_token = "your_vercel_api_token_here"

# Required: Pinecone API Key
# Get this from: https://app.pinecone.io/
pinecone_api_key = "your_pinecone_api_key_here"

# Optional: Additional API keys for backend functionality
# These will be passed to Vercel as environment variables for serverless functions

# LangChain API Key (for tracing)
langchain_api_key = "your_langchain_api_key_here"

# Voyage AI API Key (for embeddings)
voyage_api_key = "your_voyage_api_key_here"

# Google API Key
google_api_key = "your_google_api_key_here"

# Anthropic API Key
anthropic_api_key = "your_anthropic_api_key_here"

# OpenAI API Key
openai_api_key = "your_openai_api_key_here"
